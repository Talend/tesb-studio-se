<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
        STARTABLE="true">
        <SIGNATURE></SIGNATURE>
    </HEADER>

    <FAMILIES>
        <FAMILY>Exception Handling</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MIN_OUTPUT="0" MAX_OUTPUT="1" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="ERROR_HANDLER" FIELD="RADIO" NUM_ROW="2">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DEAD_LETTER" FIELD="RADIO" NUM_ROW="2">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="LOG_HANDLER" FIELD="RADIO" NUM_ROW="2">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DEAD_LETTER_URI" FIELD="TEXT"
            NUM_ROW="3" SHOW_IF="DEAD_LETTER=='true'">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_MAX_REDELIVERIES" FIELD="CHECK"
            NUM_ROW="4" SHOW_IF="(DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MAX_REDELIVERIES" FIELD="TEXT"
            NUM_ROW="4"
            SHOW_IF="((DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')) AND (USE_MAX_REDELIVERIES=='true')">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_REDELIVERY_DELAY" FIELD="CHECK"
            NUM_ROW="5" SHOW_IF="(DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="REDELIVERY_DELAY" FIELD="TEXT"
            NUM_ROW="5"
            SHOW_IF="((DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')) AND (USE_REDELIVERY_DELAY=='true')">
            <DEFAULT>1000</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_RETRY_ATTEMP_LOG_LEVEL" FIELD="CHECK"
            NUM_ROW="6" SHOW_IF="(DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="RETRY_ATTEMP_LOG_LEVEL" FIELD="CLOSED_LIST"
            NUM_ROW="6"
            SHOW_IF="((DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')) AND (USE_RETRY_ATTEMP_LOG_LEVEL=='true')">
            <ITEMS DEFAULT="INFO">
                <ITEM NAME="ERROR" VALUE="ERROR" />
                <ITEM NAME="WARN" VALUE="WARN" />
                <ITEM NAME="INFO" VALUE="INFO" />
                <ITEM NAME="DEBUG" VALUE="DEBUG" />
                <ITEM NAME="TRACE" VALUE="TRACE" />
                <ITEM NAME="OFF" VALUE="OFF" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="ASYNC_DELAYED_REDELIVERY" FIELD="CHECK"
            NUM_ROW="7" SHOW_IF="(DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_ORIGINAL_MESSAGE" FIELD="CHECK"
            NUM_ROW="8" SHOW_IF=" DEAD_LETTER=='true' ">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MORE_CONFIGURATION" FIELD="CHECK"
            NUM_ROW="9" SHOW_IF="(DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CODE" FIELD="MEMO_JAVA" REQUIRED="false"
            NUM_ROW="10" NB_LINES="9" CONTEXT="begin"
            SHOW_IF="((DEAD_LETTER=='true') OR (ERROR_HANDLER == 'true')) AND (MORE_CONFIGURATION=='true')">
            <DEFAULT>//.retryWhile(header("retry"))</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_LOG_NAME" FIELD="CHECK" NUM_ROW="11"
            SHOW_IF="LOG_HANDLER == 'true'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="LOG_NAME" FIELD="TEXT" NUM_ROW="12"
            SHOW_IF="(LOG_HANDLER == 'true') AND (USE_LOG_NAME=='true')">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_LOG_LEVEL" FIELD="CHECK" NUM_ROW="13"
            SHOW_IF="LOG_HANDLER == 'true'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="LOG_LEVEL" FIELD="CLOSED_LIST"
            NUM_ROW="14"
            SHOW_IF="(LOG_HANDLER == 'true') AND (USE_LOG_LEVEL=='true')">
            <ITEMS DEFAULT="INFO">
                <ITEM NAME="ERROR" VALUE="ERROR" />
                <ITEM NAME="WARN" VALUE="WARN" />
                <ITEM NAME="INFO" VALUE="INFO" />
                <ITEM NAME="DEBUG" VALUE="DEBUG" />
                <ITEM NAME="TRACE" VALUE="TRACE" />
                <ITEM NAME="OFF" VALUE="OFF" />
            </ITEMS>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>