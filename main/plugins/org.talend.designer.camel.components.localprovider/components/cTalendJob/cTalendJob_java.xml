<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="false">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Talend</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="FROM_REPOSITORY_JOB" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="1">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FROM_EXTERNAL_JAR" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="1">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_SELECTED_CONTEXT" FIELD="RADIO"
            NUM_ROW="4" REQUIRED="false" SHOW_IF="FROM_REPOSITORY_JOB == 'true'"
            GROUP="REPOSITORY_JOB_GROUP">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_ROUTE_CONTEXT" FIELD="RADIO"
            NUM_ROW="4" REQUIRED="false" SHOW_IF="FROM_REPOSITORY_JOB == 'true'"
            GROUP="REPOSITORY_JOB_GROUP">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_JOB_CONTEXT" FIELD="RADIO"
            NUM_ROW="4" REQUIRED="false" SHOW_IF="FROM_REPOSITORY_JOB == 'true'"
            GROUP="REPOSITORY_JOB_GROUP">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SELECTED_JOB_NAME" FIELD="ROUTE_INPUT_PROCESS_TYPE"
            NUM_ROW="6" REQUIRED="false" SHOW_IF="FROM_REPOSITORY_JOB == 'true'"
            GROUP="REPOSITORY_JOB_GROUP">
        </PARAMETER>
        <PARAMETER NAME="LIBRARY" FIELD="MODULE_LIST" NUM_ROW="9"
            REQUIRED="false" SHOW_IF="FROM_EXTERNAL_JAR == 'true'"
            GROUP="EXTERNAL_JAR_GROUP">
            <DEFAULT></DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="JOB" FIELD="TEXT" REQUIRED="false"
            NUM_ROW="10" SHOW_IF="FROM_EXTERNAL_JAR == 'true'" GROUP="EXTERNAL_JAR_GROUP">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONTEXT" FIELD="TEXT" REQUIRED="false"
            NUM_ROW="15" SHOW_IF="FROM_EXTERNAL_JAR == 'true'" GROUP="EXTERNAL_JAR_GROUP">
            <DEFAULT>"Default"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONTEXTPARAMS" FIELD="TABLE" NUM_ROW="30"
            NB_LINES="8" SHOW_IF="FROM_EXTERNAL_JAR == 'false'">
            <ITEMS>
                <ITEM NAME="PARAM_NAME_COLUMN" FIELD="CONTEXT_PARAM_NAME_LIST" />
                <ITEM NAME="PARAM_VALUE_COLUMN" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="EXTERNAL_JAR_CONTEXTPARAMS" FIELD="TABLE"
            NUM_ROW="30" NB_LINES="8" SHOW_IF="FROM_EXTERNAL_JAR == 'true'">
            <ITEMS>
                <ITEM NAME="EXTERNAL_JAR_PARAM_NAME_COLUMN" FIELD="TEXT" />
                <ITEM NAME="EXTERNAL_JAR_PARAM_VALUE_COLUMN" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="PROPAGATE_HEADER" FIELD="CHECK"
            NUM_ROW="20">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT NAME="${tesb-camel-talendjob}" MODULE="${tesb-camel-talendjob}"
                MVN="${mvn-camel-talendjob}"
                UrlPath="platform:/plugin/org.talend.designer.camel.components.localprovider/lib/${tesb-camel-talendjob}"
                REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

    <PLUGINDEPENDENCIES>
        <PLUGINDEPENDENCY ID="org.talend.model" />
    </PLUGINDEPENDENCIES>

</COMPONENT>