<COMPONENT>
    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Connectivity/Messaging</FAMILY>
        <FAMILY>Connectivity/Internet of Things</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="TYPE" FIELD="CLOSED_LIST" NUM_ROW="2"
            REQUIRED="true" READONLY="false" GROUP="URI_GROUP">
            <ITEMS DEFAULT="QUEUE">
                <ITEM NAME="QUEUE" VALUE="queue" />
                <ITEM NAME="TOPIC" VALUE="topic" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="DESTINATION" READONLY="false" FIELD="TEXT"
            REQUIRED="true" NUM_ROW="4" GROUP="URI_GROUP">
            <DEFAULT>"name"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONNECTION_FACOTRY_CONFIGURATION"
            FIELD="ROUTE_COMPONENT_TYPE" FILTER="cMQConnectionFactory&amp;MQ_TYPE == 'AMQP'"
            NUM_ROW="12">
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="URI_OPTIONS" FIELD="TABLE" NUM_ROW="7"
            NB_LINES="5">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS />
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>