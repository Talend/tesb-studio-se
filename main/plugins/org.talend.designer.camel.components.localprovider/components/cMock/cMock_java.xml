<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20160412A"
        STARTABLE="true">
        <SIGNATURE></SIGNATURE>
    </HEADER>

    <FAMILIES>
        <FAMILY>Testing</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="TEST_HEADER" FIELD="CHECK" REQUIRED="true"
            NUM_ROW="10" GROUP="HEADER">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_HEADER_CONTEXT" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="20" SHOW_IF="TEST_HEADER=='true'"
            GROUP="HEADER">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_HEADER_USE_CONTEXT" FIELD="TEXT"
            NUM_ROW="25"
            SHOW_IF="(TEST_HEADER_CONTEXT=='true') AND (TEST_HEADER=='true')"
            GROUP="HEADER">
        </PARAMETER>
        <PARAMETER NAME="TEST_HEADER_TABLE" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="30" SHOW_IF="TEST_HEADER=='true'"
            GROUP="HEADER">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_HEADER_USE_TABLE" FIELD="TABLE"
            NUM_ROW="35" NB_LINES="5"
            SHOW_IF="(TEST_HEADER_TABLE=='true') AND (TEST_HEADER=='true')"
            GROUP="HEADER">
            <ITEMS>
                <ITEM NAME="HEADER_NAMES" VALUE="&quot;HeaderName&quot;" />
                <ITEM NAME="HEADER_VALUES" VALUE="&quot;HeaderValue&quot;" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="TEST_BODY" FIELD="CHECK" REQUIRED="true"
            NUM_ROW="40" GROUP="BODY">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_BODY_CONTEXT" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="50" SHOW_IF="TEST_BODY=='true'"
            GROUP="BODY">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_BODY_USE_CONTEXT" FIELD="TEXT"
            NUM_ROW="55"
            SHOW_IF="(TEST_BODY_CONTEXT=='true') AND (TEST_BODY=='true')"
            GROUP="BODY">
        </PARAMETER>
        <PARAMETER NAME="TEST_BODY_TABLE" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="60" SHOW_IF="TEST_BODY=='true'"
            GROUP="BODY">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_BODY_USE_TABLE" FIELD="TABLE"
            NUM_ROW="65" NB_LINES="5"
            SHOW_IF="(TEST_BODY_TABLE=='true') AND (TEST_BODY=='true')"
            GROUP="BODY">
            <ITEMS>
                <ITEM NAME="BODY_VALUES" VALUE="&quot;BodyValue&quot;" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="TEST_COUNT" FIELD="CHECK" REQUIRED="true"
            NUM_ROW="1">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MESSAGE_COUNT" FIELD="TEXT" NUM_ROW="1"
            SHOW_IF="TEST_COUNT=='true'">
            <DEFAULT>1</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_WAIT_TIME" FIELD="TEXT" NUM_ROW="67"
            SHOW_IF="(TEST_COUNT=='true') OR (TEST_BODY=='true') OR (TEST_HEADER=='true')">
            <DEFAULT>2000</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TEST_SIMULATE" FIELD="CHECK"
            REQUIRED="true" NUM_ROW="70" SHOW_IF="#LINK@CONNECTOR.OUT.ANY &gt; '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PROCESSOR_ENDPOINT" FIELD="ROUTE_COMPONENT_TYPE"
            FILTER="cProcessor" NUM_ROW="75"
            SHOW_IF="(TEST_SIMULATE=='true') AND (#LINK@CONNECTOR.OUT.ANY &gt; '0')">
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-test" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>