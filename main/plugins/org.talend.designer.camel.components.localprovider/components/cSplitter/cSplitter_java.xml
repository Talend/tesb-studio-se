<!DOCTYPE document [
<!ENTITY languages SYSTEM "../common/languages.xml">
<!ENTITY namespaces SYSTEM "../common/namespaces.xml">
]>
<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="false">
        <SIGNATURE />
    </HEADER>
    <FAMILIES>

        <FAMILY>Routing</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR NAME="SPLIT" CTYPE="ROUTE" MIN_INPUT="0"
            MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0" COLOR="7171FF" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MIN_INPUT="0"
            MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="LANGUAGES" FIELD="CLOSED_LIST"
            REQUIRED="false" NUM_ROW="3">
            <ITEMS DEFAULT="NONE">
                &languages;
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="EXPRESSION" FIELD="TEXT" NUM_ROW="6"
            REQUIRED="true" GROUP="CORRELATION">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_RESULT_CLASS_TYPE" FIELD="CHECK"
            REQUIRED="true" NUM_ROW="7" GROUP="CORRELATION" SHOW_IF="LANGUAGES=='XPATH'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="RESULT_CLASS_TYPE" FIELD="TEXT"
            NUM_ROW="7" REQUIRED="false" GROUP="CORRELATION"
            SHOW_IF="(USE_RESULT_CLASS_TYPE=='true') and (LANGUAGES=='XPATH')">
            <DEFAULT>java.lang.String.class</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_NAMESPACES" FIELD="CHECK"
            REQUIRED="true" NUM_ROW="8" GROUP="CORRELATION" SHOW_IF="LANGUAGES=='XPATH'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="NAMESPACES" FIELD="TABLE" NUM_ROW="9"
            NB_LINES="5" REQUIRED="true" GROUP="CORRELATION"
            SHOW_IF="(LANGUAGES=='XPATH') AND (USE_NAMESPACES == 'true')">
            <ITEMS>
                <ITEM NAME="PREFIX" VALUE="" />
                <ITEM NAME="URI" VALUE="" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="USE_STRATEGY" FIELD="CHECK" REQUIRED="true"
            NUM_ROW="10">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="AGGREGATION_STRATEGY" FIELD="TEXT"
            NUM_ROW="10" REQUIRED="false" SHOW_IF="USE_STRATEGY=='true'">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PARALLET_PROCESSING" FIELD="CHECK"
            NUM_ROW="12" GROUP="ARGUMENTS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="STOP_ON_EXCEPTION" FIELD="CHECK"
            NUM_ROW="14" GROUP="ARGUMENTS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="STREAMING" FIELD="CHECK" NUM_ROW="16"
            GROUP="ARGUMENTS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SHARE_UNIT_OF_WORK" FIELD="CHECK"
            NUM_ROW="18" GROUP="ARGUMENTS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TIMEOUT" FIELD="TEXT" NUM_ROW="20"
            GROUP="ARGUMENTS">
            <DEFAULT></DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-groovy" REQUIRED_IF="LANGUAGES == 'GROOVY'" />
            <!-- camel-script -->
            <IMPORT NAME="camel-script" MODULE="camel-script-${camel.version}.jar"
                MVN="${mvn-camel-script}"
                UrlPath="platform:/plugin/org.talend.designer.camel.components.localprovider/lib/camel-script-${camel.version}.jar"
                REQUIRED_IF="LANGUAGES == 'JAVASCRIPT'" />
            <IMPORT NAME="talend-camel-jsonpath"
                MODULE="camel-jsonpath-alldep-${camel.version}.jar"
                MVN="mvn:org.talend.libraries/camel-jsonpath-alldep-${camel.version}/6.0.0"
                UrlPath="platform:/plugin/org.talend.libraries.camel.alldeps/lib/camel-jsonpath-alldep-${camel.version}.jar"
                REQUIRED_IF="LANGUAGES == 'JSONPATH'" />
            <IMPORT NAME="talend-camel-saxon"
                MODULE="camel-saxon-alldep-${camel.version}.jar"
                MVN="mvn:org.talend.libraries/camel-saxon-alldep-${camel.version}/6.0.0"
                UrlPath="platform:/plugin/org.talend.libraries.camel.alldeps/lib/camel-saxon-alldep-${camel.version}.jar"
                REQUIRED_IF="LANGUAGES == 'XQUERY'" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>