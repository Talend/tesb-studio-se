<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Testing</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <!-- DATASET -->
        <PARAMETER NAME="DATASET_NAME" FIELD="TEXT" NUM_ROW="16">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DATASET_NAME_NOTE" FIELD="LABEL"
            NUM_ROW="16">
            <DEFAULT>(Registered Dataset Bean ID)</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PRODUCE_DELAY" FIELD="TEXT" NUM_ROW="19">
            <DEFAULT>3</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONSUME_DELAY" FIELD="TEXT" NUM_ROW="19">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PRELOAD_SIZE" FIELD="TEXT" NUM_ROW="28">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="INITIAL_DELAY" FIELD="TEXT" NUM_ROW="28">
            <DEFAULT>1000</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MIN_RATE" FIELD="TEXT" NUM_ROW="37">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="REGISTER_DATASET_BEAN" FIELD="CHECK"
            NUM_ROW="40">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="BEAN_CLASS" FIELD="TEXT" NUM_ROW="42"
            SHOW_IF=" REGISTER_DATASET_BEAN == 'true'">
            <DEFAULT>org.apache.camel.component.dataset.SimpleDataSet
            </DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="BEAN_ARGUMENTS" FIELD="TABLE"
            NUM_ROW="44" NB_LINES="3" SHOW_IF=" REGISTER_DATASET_BEAN == 'true'">
            <ITEMS>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="DATASET_REGISTER_NOTE" FIELD="LABEL"
            NUM_ROW="46" SHOW_IF=" REGISTER_DATASET_BEAN == 'true'">
            <DEFAULT>*Note: Please use cBeanRegister to register Bean
                for more complex case</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>