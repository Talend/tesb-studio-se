<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Connectivity/Internet</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="PROTOCOLS" FIELD="CLOSED_LIST"
            NUM_ROW="1">
            <ITEMS DEFAULT="smtp">
                <ITEM NAME="smtp" VALUE="smtp" />
                <ITEM NAME="pop3" VALUE="pop3" />
                <ITEM NAME="imap" VALUE="imap" />
                <ITEM NAME="smtps" VALUE="smtps" />
                <ITEM NAME="pop3s" VALUE="pop3s" />
                <ITEM NAME="imaps" VALUE="imaps" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="2">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="2">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="3">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="3">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SUBJECT" FIELD="TEXT" NUM_ROW="4">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONTENTTYPE" FIELD="TEXT" NUM_ROW="4">
            <DEFAULT>"text/plain"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FROM" FIELD="TEXT" NUM_ROW="5">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TO" FIELD="TEXT" NUM_ROW="5">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CC" FIELD="TEXT" NUM_ROW="6">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="BCC" FIELD="TEXT" NUM_ROW="6">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="ARGUMENTS" FIELD="TABLE" NUM_ROW="10"
            NB_LINES="8">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-mail" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>