<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Orchestration</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="0"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="0" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="0"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <!-- TIMER -->
        <PARAMETER NAME="PERIOD" FIELD="TEXT" NUM_ROW="13">
            <DEFAULT>1000</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="REPEATCOUNT" FIELD="TEXT" NUM_ROW="15">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DELAY" FIELD="TEXT" NUM_ROW="16">
            <DEFAULT>1000</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FIXEDRATE" FIELD="CHECK" NUM_ROW="17">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DAEMON" FIELD="CHECK" NUM_ROW="19">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SET_TIME" FIELD="CHECK" NUM_ROW="21">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TIME" FIELD="TEXT" NUM_ROW="22"
            SHOW_IF="SET_TIME == 'true' ">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="22"
            SHOW_IF="SET_TIME == 'true' ">
            <DEFAULT>(yyyy-MM-dd HH:mm:ss | yyyy-MM-dd'T'HH:mm:ss)
            </DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>