<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Connectivity/Internet</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="TYPE" FIELD="CLOSED_LIST" NUM_ROW="4"
            REQUIRED="true" GROUP="ARGUMENTS">
            <ITEMS DEFAULT="FTP">
                <ITEM NAME="FTP" VALUE="ftp" />
                <ITEM NAME="FTPS" VALUE="ftps" />
                <ITEM NAME="SFTP" VALUE="sftp" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="SERVER" FIELD="TEXT" REQUIRED="true"
            NUM_ROW="5" GROUP="ARGUMENTS">
            <DEFAULT>"localhost"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="6"
            GROUP="ARGUMENTS">
            <DEFAULT>"21"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="7"
            GROUP="ARGUMENTS">
            <DEFAULT>"username"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="8"
            GROUP="ARGUMENTS">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DIRECTORY" FIELD="TEXT" NUM_ROW="9"
            GROUP="ARGUMENTS">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="ADVARGUMENTS" FIELD="TABLE" NUM_ROW="10"
            NB_LINES="5">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-ftp" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>