<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="false" SCHEMA_AUTO_PROPAGATE="false"
        DATA_AUTO_PROPAGATE="false">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Custom</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MAX_INPUT="0" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="0" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="0"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="0"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="ID" FIELD="TEXT" REQUIRED="true"
            NUM_ROW="1">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SIMPLE_CONFIG" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="2">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONFIG_BY_CODE" FIELD="RADIO"
            REQUIRED="true" NUM_ROW="2">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CLASS_QUALIFIED_NAME" FIELD="TEXT"
            REQUIRED="true" NUM_ROW="4" SHOW_IF="SIMPLE_CONFIG == 'true' ">
            <DEFAULT>java.lang.Object</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SPECIFY_ARGUMENTS" FIELD="CHECK"
            REQUIRED="true" NUM_ROW="7" SHOW_IF="SIMPLE_CONFIG == 'true' ">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ARGUMENTS" FIELD="TABLE" NB_LINES="8"
            NUM_ROW="10"
            SHOW_IF="(SIMPLE_CONFIG == 'true') AND (SPECIFY_ARGUMENTS == 'true') ">
            <ITEMS>
                <ITEM NAME="VALUE" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="IMPORT" FIELD="MEMO_IMPORT" REQUIRED="false"
            NUM_ROW="11" NB_LINES="2" SHOW_IF="CONFIG_BY_CODE == 'true' ">
            <DEFAULT>//import java.util.List;</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="NOTE" FIELD="LABEL" REQUIRED="false"
            NUM_ROW="18" SHOW_IF="CONFIG_BY_CODE == 'true' ">
            <DEFAULT>Note: Provide own codes to specify *beanInstance* variable</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CODE" FIELD="MEMO_JAVA" REQUIRED="false"
            NUM_ROW="12" NB_LINES="9" CONTEXT="begin"
            SHOW_IF="CONFIG_BY_CODE == 'true' ">
            <DEFAULT>// beanInstance = ....;</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION />

    <RETURNS />

