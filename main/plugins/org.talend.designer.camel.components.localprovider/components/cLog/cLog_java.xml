<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="false">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Miscellaneous</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="LOGGING_CATEGORY" FIELD="TEXT"
            NUM_ROW="7">
            <DEFAULT>"__NAME__.__NODE_UNIQUE_NAME__"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="LEVEL_TYPE" FIELD="CLOSED_LIST"
            NUM_ROW="12">
            <ITEMS DEFAULT="WARN">
                <!-- <ITEM NAME="FATAL" VALUE="FATAL" /> -->
                <ITEM NAME="ERROR" VALUE="ERROR" />
                <ITEM NAME="WARN" VALUE="WARN" />
                <ITEM NAME="INFO" VALUE="INFO" />
                <ITEM NAME="DEBUG" VALUE="DEBUG" />
                <ITEM NAME="TRACE" VALUE="TRACE" />
                <ITEM NAME="OFF" VALUE="OFF" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="NOTHING" FIELD="RADIO" NUM_ROW="13"
            GROUP="OPTIONS">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="GROUP_SIZE" FIELD="RADIO" NUM_ROW="14"
            GROUP="OPTIONS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SIZE" FIELD="TEXT" NUM_ROW="15"
            GROUP="OPTIONS" SHOW_IF="GROUP_SIZE=='true'">
            <DEFAULT>1</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="GROUP_INTERVAL" FIELD="RADIO"
            NUM_ROW="16" GROUP="OPTIONS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="INTERVAL" FIELD="TEXT" NUM_ROW="17"
            GROUP="OPTIONS" SHOW_IF="GROUP_INTERVAL=='true'">
            <DEFAULT>1</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="DELAY" FIELD="TEXT" NUM_ROW="18"
            GROUP="OPTIONS" SHOW_IF="GROUP_INTERVAL=='true'">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ACTIVE_ONLY" FIELD="CHECK" NUM_ROW="19"
            GROUP="OPTIONS" SHOW_IF="GROUP_INTERVAL=='true'">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FORMAT" FIELD="RADIO" NUM_ROW="20"
            GROUP="OPTIONS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ARGUMENTS" FIELD="TABLE" NUM_ROW="21"
            NB_LINES="8" GROUP="OPTIONS" SHOW_IF="FORMAT=='true'">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="USE_LOGEIP" FIELD="RADIO" NUM_ROW="22"
            GROUP="OPTIONS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MESSAGE" FIELD="TEXT" NUM_ROW="23"
            GROUP="OPTIONS" SHOW_IF="USE_LOGEIP=='true'">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION />

    <RETURNS />

</COMPONENT>