<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Exception Handling</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MAX_INPUT="0" MAX_OUTPUT="1"
            MIN_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="EXCEPTIONS" FIELD="TABLE" NUM_ROW="10"
            NB_LINES="5">
            <ITEMS>
                <ITEM NAME="EXCEPTION" VALUE="java.lang.Exception"
                    FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="SET_MAX_REDELIVERY" FIELD="CHECK"
            NUM_ROW="12">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MAX_REDELIVERY" FIELD="TEXT" NUM_ROW="12"
            SHOW_IF="SET_MAX_REDELIVERY=='true'">
            <DEFAULT>2</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ASYNC_REDELIVERY" FIELD="CHECK"
            NUM_ROW="15">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="NONE" FIELD="RADIO" NUM_ROW="19"
            GROUP="EXCEPTION_BEHAVIOUR">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="HANDLE" FIELD="RADIO" NUM_ROW="20"
            GROUP="EXCEPTION_BEHAVIOUR">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONTINUE" FIELD="RADIO" NUM_ROW="21"
            GROUP="EXCEPTION_BEHAVIOUR">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_ORIGINAL_MESSAGE" FIELD="CHECK"
            NUM_ROW="30">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION />

    <RETURNS />

</COMPONENT>