<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<COMPONENT>

    <HEADER AUTHOR="Talend" COMPATIBILITY="ALL" PLATEFORM="ALL"
        RELEASE_DATE="20130311A" SERIAL="" STARTABLE="true" STATUS="ALPHA"
        VERSION="0.1">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Core</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"
            MIN_INPUT="0" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <!-- USE EXIST ENDPOINT(Provider) -->
        <PARAMETER NAME="SELECT_FROM_EXISTING" FIELD="RADIO"
            NUM_ROW="1" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="INPUT_ENDPOINT_NAME" FIELD="RADIO"
            NUM_ROW="1" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="EXIST_ENDPOINT" FIELD="ROUTE_COMPONENT_TYPE"
            FILTER="cDirectVM" NUM_ROW="4"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0') AND (SELECT_FROM_EXISTING == 'true')">
        </PARAMETER>
        <PARAMETER NAME="ENDPOINT_NAME" FIELD="TEXT" NUM_ROW="4"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0') OR (INPUT_ENDPOINT_NAME == 'true') ">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="BLOCK" FIELD="CHECK" NUM_ROW="12"
            SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="TIMEOUT" FIELD="TEXT" NUM_ROW="18"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0') AND (BLOCK == 'true') ">
            <DEFAULT>30000</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS />

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>