<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<COMPONENT>

    <HEADER AUTHOR="Talend" COMPATIBILITY="ALL" PLATEFORM="ALL"
        RELEASE_DATE="20130311A" SERIAL="" STARTABLE="true" STATUS="ALPHA"
        VERSION="0.1">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Core</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"
            MIN_INPUT="0" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <!-- USE EXIST ENDPOINT(Provider) -->
        <PARAMETER NAME="INPUT_ENDPOINT_NAME" FIELD="CHECK"
            NUM_ROW="1" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="EXIST_ENDPOINT" FIELD="ROUTE_COMPONENT_TYPE"
            FILTER="cVM" NUM_ROW="5"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0') AND (INPUT_ENDPOINT_NAME == 'false')">
        </PARAMETER>
        <!-- CREATE NEW ENDPOINT -->
        <PARAMETER NAME="NAME" FIELD="TEXT" NUM_ROW="4"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0') OR (INPUT_ENDPOINT_NAME == 'true')">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SPECIFY_SIZE" FIELD="CHECK" NUM_ROW="5"
            SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="SIZE" FIELD="TEXT" NUM_ROW="5"
            SHOW_IF="(SPECIFY_SIZE == 'true') AND (#LINK@CONNECTOR.IN.ANY == '0')">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONCURRENT_CONSUMERS" FIELD="TEXT"
            NUM_ROW="15" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>1</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="WAIT_FOR_TASK_TO_COMPLETE" FIELD="CLOSED_LIST"
            REQUIRED="false" NUM_ROW="20" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <ITEMS DEFAULT="IFREPLYEXPECTED">
                <ITEM NAME="ALWAYS" VALUE='"Always"' />
                <ITEM NAME="NEVER" VALUE='"Never"' />
                <ITEM NAME="IFREPLYEXPECTED" VALUE='"IfReplyExpected"' />
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="TIMEOUT" FIELD="TEXT" NUM_ROW="23"
            SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>30000</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="MULTIPLE_CONSUMERS" FIELD="CHECK"
            NUM_ROW="25" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="LIMIT_CONCURRENT_CONSUMERS" FIELD="CHECK"
            NUM_ROW="30" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="BLOCK_WHEN_FULL" FIELD="CHECK"
            NUM_ROW="35" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="POLLTIMEOUT" FIELD="TEXT" NUM_ROW="40"
            SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>1000</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="ADVARGUMENTS" FIELD="TABLE" NUM_ROW="10"
            NB_LINES="5" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>
