<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Connectivity/File</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="URI" FIELD="DIRECTORY" NUM_ROW="4"
            REQUIRED="true">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="NOOP" FIELD="CHECK" NUM_ROW="5"
            GROUP="ARGUMENTS">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FLATTEN" FIELD="CHECK" NUM_ROW="6"
            GROUP="ARGUMENTS">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="AUTOCREATE" FIELD="CHECK" NUM_ROW="7"
            GROUP="ARGUMENTS">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="BUFFER_SIZE" FIELD="TEXT" NUM_ROW="8"
            GROUP="ARGUMENTS">
            <DEFAULT>"128"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE"
            NUM_ROW="9" REPOSITORY_VALUE="ENCODING" GROUP="ARGUMENTS">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FILENAME" FIELD="TEXT" NUM_ROW="10"
            GROUP="ARGUMENTS">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="ADVARGUMENTS" FIELD="TABLE" NUM_ROW="10"
            NB_LINES="5">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>