<COMPONENT>

    <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
        COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
        STARTABLE="true">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Asure</FAMILY>
        <FAMILY>Connectivity/File</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
            MAX_OUTPUT="1" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
	
	    <PARAMETER NAME="STORAGE_ACCOUNT_NAME" FIELD="TEXT" NUM_ROW="10"
            REQUIRED="true" GROUP="STORAGE_CONNECTION_GROUP" >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="STORAGE_ACCESS_KEY" FIELD="PASSWORD" NUM_ROW="20" 
            REQUIRED="true" GROUP="STORAGE_CONNECTION_GROUP" >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
	    <PARAMETER NAME="STORAGE_CONTAINER_NAME" FIELD="TEXT" NUM_ROW="30"
            REQUIRED="true" GROUP="STORAGE_CONNECTION_GROUP" >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
	
        <PARAMETER NAME="BLOB_TYPE" FIELD="CLOSED_LIST"
            RAW="true" NUM_ROW="40" REQUIRED="true" GROUP="URI_GROUP">
            <ITEMS DEFAULT="BlockBlob">
                <ITEM NAME="BlockBlob" VALUE="blockblob" />
				<ITEM NAME="PageBlob" VALUE="pageblob"/>
                <ITEM NAME="AppendBlob" VALUE="appendblob" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER NAME="OPERATION" FIELD="CLOSED_LIST"
            RAW="true" NUM_ROW="50" REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0')">
            <ITEMS DEFAULT="listBlobs">
                <ITEM NAME="getBlob" VALUE="getBlob" />
                <ITEM NAME="deleteBlob" VALUE="deleteBlob" />
				<ITEM NAME="listBlobs" VALUE="listBlobs" />
				<ITEM NAME="uploadBlobBlocks" VALUE="uploadBlobBlocks" SHOW_IF="(BLOB_TYPE == 'blockblob')"  />
				<ITEM NAME="commitBlobBlockList" VALUE="commitBlobBlockList" SHOW_IF="(BLOB_TYPE == 'blockblob')"  />
				<ITEM NAME="getBlobBlockList" VALUE="getBlobBlockList" SHOW_IF="(BLOB_TYPE == 'blockblob')"  />
				<ITEM NAME="createAppendBlob" VALUE="createAppendBlob" SHOW_IF="(BLOB_TYPE == 'appendblob')"  />
				<ITEM NAME="createPageBlob" VALUE="createPageBlob" SHOW_IF="(BLOB_TYPE == 'pageblob')"  />
				<ITEM NAME="resizePageBlob" VALUE="resizePageBlob" SHOW_IF="(BLOB_TYPE == 'pageblob')"  />
				<ITEM NAME="clearPageBlob" VALUE="clearPageBlob" SHOW_IF="(BLOB_TYPE == 'pageblob')"  />
				<ITEM NAME="getPageBlobRanges" VALUE="getPageBlobRanges" SHOW_IF="(BLOB_TYPE == 'pageblob')"  />
				<ITEM NAME="updatePageBlob" VALUE="updatePageBlob" SHOW_IF="(BLOB_TYPE == 'pageblob')"  />
				<ITEM NAME="updateAppendBlob" VALUE="updateAppendBlob" SHOW_IF="(BLOB_TYPE == 'appendblob')"  />
				<ITEM NAME="updateBlockBlob" VALUE="updateBlockBlob" SHOW_IF="(BLOB_TYPE == 'blockblob')"  />								
            </ITEMS>
        </PARAMETER>
        <PARAMETER NAME="BLOB_NAME" FIELD="TEXT" NUM_ROW="50"
            REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(OPERATION == 'getBlob') OR (OPERATION == 'deleteBlob') OR (OPERATION == 'updateBlockBlob') OR (OPERATION == 'uploadBlobBlocks') OR (OPERATION == 'createAppendBlob') OR (OPERATION == 'updateAppendBlob')
				OR (OPERATION == 'createPageBlob') OR (OPERATION == 'updatePageBlob') OR (OPERATION == 'resizePageBlob') OR (OPERATION == 'clearPageBlob') OR (OPERATION == 'getPageBlobRanges')">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="BLOB_OFFSET" FIELD="TEXT" NUM_ROW="60"
            REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(OPERATION == 'uploadBlobBlocks') OR (OPERATION == 'deleteBlob') OR (OPERATION == 'updateBlockBlob') OR (OPERATION == 'uploadBlobBlocks') OR (BLOB_TYPE == 'AppendBlob')
				OR (OPERATION == 'createPageBlob') OR (OPERATION == 'updatePageBlob') OR (OPERATION == 'resizePageBlob') OR (OPERATION == 'clearPageBlob') OR (OPERATION == 'getPageBlobRanges')">
            <DEFAULT>"0"</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="DATA_LENGTH" FIELD="TEXT" NUM_ROW="70"
            REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(OPERATION == 'uploadBlobBlocks') OR (OPERATION == 'deleteBlob') OR (OPERATION == 'updateBlockBlob') OR (OPERATION == 'uploadBlobBlocks') OR (BLOB_TYPE == 'AppendBlob')  
				OR (OPERATION == 'createPageBlob') OR (OPERATION == 'updatePageBlob') OR (OPERATION == 'resizePageBlob') OR (OPERATION == 'clearPageBlob') OR (OPERATION == 'getPageBlobRanges')">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="FILE_DIR" FIELD="TEXT" NUM_ROW="80"
            REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(OPERATION == 'getBlob')">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="CLOSE_STREAM_AFTER_READ" FIELD="CHECK" NUM_ROW="90"
            REQUIRED="true" GROUP="URI_GROUP" >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="CLOSE_STREAM_AFTER_WRITE" FIELD="CHECK" NUM_ROW="95"
            REQUIRED="true" GROUP="URI_GROUP"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0')">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="PUBLIC_FOR_READ" FIELD="CHECK" NUM_ROW="100"
            REQUIRED="true" GROUP="URI_GROUP">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="BRIDGE_ERROR_HANDLER" FIELD="CHECK" NUM_ROW="105"
            REQUIRED="true" GROUP="URI_GROUP"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="USE_FLAT_LISTING" FIELD="CHECK" NUM_ROW="107"
            REQUIRED="true" GROUP="URI_GROUP"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0') AND ((OPERATION == 'listBlobs')  OR (OPERATION == 'getBlobBlockList') OR (OPERATION == 'commitBlobBlockList'))">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="SYNCHRONOUS" FIELD="CHECK" NUM_ROW="108"
            REQUIRED="true" GROUP="URI_GROUP">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

		
		
		<!--
        <PARAMETER NAME="EXCEPTION_HANDLER" FIELD="CHECK" NUM_ROW="105"
            REQUIRED="true" GROUP="URI_GROUP"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="EXCHANGE_PATTERN" FIELD="CHECK" NUM_ROW="106"
            REQUIRED="true" GROUP="URI_GROUP"
            SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>		
		-->
		
        <PARAMETER NAME="STREAM_READ_SIZE" FIELD="TEXT" NUM_ROW="110"
            REQUIRED="true" GROUP="URI_GROUP">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="STREAM_WRITE_SIZE" FIELD="TEXT" NUM_ROW="115"
            REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0') AND ((OPERATION == 'uploadBlobBlocks')  OR (OPERATION == 'updateBlockBlob') OR (OPERATION == 'uploadBlobBlocks') OR (OPERATION == 'createAppendBlob') OR (OPERATION == 'updateAppendBlob')
				OR (OPERATION == 'createPageBlob') OR (OPERATION == 'updatePageBlob') OR (OPERATION == 'resizePageBlob') OR (OPERATION == 'clearPageBlob'))" >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>		
		
		<PARAMETER NAME="BLOB_PREFIX" FIELD="TEXT" NUM_ROW="120"
            REQUIRED="true" GROUP="URI_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0')" >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
		
		<!-- ACCESS CONDITIONS -->
		
		<PARAMETER NAME="SET_ACCESS_CONDITIONS" FIELD="CHECK" NUM_ROW="150"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0')" >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="IF_APPEND_POSITION_EQUAL_CONDITION" FIELD="TEXT" NUM_ROW="151"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>	
		
		<PARAMETER NAME="IF_MATCH_CONDITION" FIELD="TEXT" NUM_ROW="152"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="IF_NONE_MATCH_CONDITION" FIELD="TEXT" NUM_ROW="153"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>		
		
		<PARAMETER NAME="IF_MODIFIED_SINCE_CONDITION" FIELD="TEXT" NUM_ROW="154"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>		
		
		<PARAMETER NAME="IF_NOT_MODIFIED_SINCE_CONDITION" FIELD="TEXT" NUM_ROW="155"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>

		<PARAMETER NAME="IF_SEQUENCE_NUMBER_LESS_THAN_OR_EQUAL_CONDITION" FIELD="TEXT" NUM_ROW="156"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="IF_SEQUENCE_NUMBER_LESS_THAN_CONDITION" FIELD="TEXT" NUM_ROW="157"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="IF_SEQUENCE_NUMBER_EQUAL_CONDITION" FIELD="TEXT" NUM_ROW="158"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="LEASE_CONDITION" FIELD="TEXT" NUM_ROW="159"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="IF_EXISTS_CONDITION" FIELD="TEXT" NUM_ROW="160"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		
        <PARAMETER NAME="IF_NOT_EXISTS_CONDITION" FIELD="TEXT" NUM_ROW="161"
            REQUIRED="false" GROUP="ACCESS_CONDITIONS_GROUP" SHOW_IF="(SET_ACCESS_CONDITIONS == 'true')" >
            <DEFAULT></DEFAULT>
        </PARAMETER>


		<PARAMETER NAME="SET_BLOB_METADATA" FIELD="CHECK" NUM_ROW="180"
            REQUIRED="false" GROUP="BLOB_METADATA_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0')" >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
		
		<PARAMETER NAME="BLOB_METADATA" FIELD="TABLE" NUM_ROW="181" NB_LINES="7" GROUP="BLOB_METADATA_GROUP" REQUIRED="false" SHOW_IF="((#LINK@CONNECTOR.IN.ANY &gt; '0') AND (SET_BLOB_METADATA == 'true' ))">
			<ITEMS>
				<ITEM NAME="BLOB_METADATA_KEY" FIELD="OPENED_LIST"/>
				<ITEM NAME="BLOB_METADATA_VALUE" />
			</ITEMS>
		</PARAMETER>		

		<PARAMETER NAME="MESSAGE_HEADERS" FIELD="TABLE" NUM_ROW="200" NB_LINES="7" GROUP="MESSAGE_GROUP" REQUIRED="false" SHOW_IF="(#LINK@CONNECTOR.IN.ANY &gt; '0')">
			<ITEMS>
				<ITEM NAME="MESSAGE_HEADER" FIELD="OPENED_LIST">
					<ITEMS DEFAULT="CamelFileName">
						<ITEM NAME="CamelFileName" FIELD="TEXT" VALUE="CamelFileName"/>
					</ITEMS>
				</ITEM>
				<ITEM NAME="MESSAGE_HEADER_VALUE" />
			</ITEMS>
		</PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="URI_OPTIONS" FIELD="TABLE" NUM_ROW="7"
            NB_LINES="5">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT" />
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

