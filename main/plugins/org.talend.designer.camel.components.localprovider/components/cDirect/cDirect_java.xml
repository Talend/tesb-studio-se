<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<COMPONENT>

    <HEADER AUTHOR="Talend" COMPATIBILITY="ALL" PLATEFORM="ALL"
        RELEASE_DATE="20130308A" SERIAL="" STARTABLE="true" STATUS="ALPHA"
        VERSION="0.1">
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>Core</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"
            MIN_INPUT="0" MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1"
            MAX_OUTPUT="0" />
        <CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1"
            MAX_OUTPUT="0" />
    </CONNECTORS>

    <PARAMETERS>
        <!-- USE EXIST ,provider -->
        <PARAMETER NAME="EXIST_ENDPOINT" FIELD="ROUTE_COMPONENT_TYPE"
            FILTER="cDirect" NUM_ROW="6" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
        </PARAMETER>
        <!-- CREATE NEW -->
        <PARAMETER NAME="NAME" FIELD="TEXT" NUM_ROW="4"
            SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'" REQUIRED_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
    </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="ADVARGUMENTS" FIELD="TABLE" NUM_ROW="10"
            NB_LINES="5" SHOW="false">
            <ITEMS>
                <ITEM NAME="NAME" FIELD="TEXT">
                </ITEM>
                <ITEM NAME="VALUE" />
            </ITEMS>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
        </IMPORTS>
    </CODEGENERATION>

    <RETURNS />

</COMPONENT>